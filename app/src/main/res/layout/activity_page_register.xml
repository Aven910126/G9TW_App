<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/messageimage_1653478110059"
    tools:context=".page_register">

    <EditText
        android:id="@+id/rusername"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="233dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="29dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/rcall"
        android:layout_width="191dp"
        android:layout_height="38dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="27dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/rusername"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="32dp"
        android:text="電子信箱:"
        app:layout_constraintBottom_toTopOf="@+id/rphone"
        app:layout_constraintEnd_toStartOf="@+id/riptmail"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="65dp"
        android:text="確認密碼:"
        app:layout_constraintBottom_toTopOf="@+id/rbtn"
        app:layout_constraintEnd_toStartOf="@+id/rcpwd"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/riptpwd" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="87dp"
        android:layout_marginEnd="16dp"
        android:text="電話:"
        app:layout_constraintBaseline_toBaselineOf="@+id/rphone"
        app:layout_constraintEnd_toStartOf="@+id/rphone"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/rcpwd"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginTop="98dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="97dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/rbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/rphone"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="17dp"
        android:ems="10"
        android:inputType="phone"
        android:maxLength="10"
        app:layout_constraintBottom_toTopOf="@+id/riptpwd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="87dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="20dp"
        android:text="姓名:"
        app:layout_constraintBottom_toBottomOf="@+id/rusername"
        app:layout_constraintEnd_toStartOf="@+id/rusername"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/riptpwd"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="36dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/rphone"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="87dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="42dp"
        android:text="稱謂:"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/rcall"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rusername" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="87dp"
        android:layout_marginEnd="18dp"
        android:text="密碼:"
        app:layout_constraintBaseline_toBaselineOf="@+id/riptpwd"
        app:layout_constraintEnd_toStartOf="@+id/riptpwd"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/riptmail"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toTopOf="@+id/rphone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/rcall"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/rbtn"
        android:layout_width="170dp"
        android:layout_height="0dp"
        android:layout_marginBottom="38dp"
        android:background="@drawable/login1_"
        android:fadeScrollbars="true"
        android:text="REG"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rdeviceCode" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="4dp"
        android:text="機器碼"
        app:layout_constraintBottom_toBottomOf="@+id/rdeviceCode"
        app:layout_constraintEnd_toStartOf="@+id/rdeviceCode"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/rdeviceCode"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="596dp"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="9dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/rbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>